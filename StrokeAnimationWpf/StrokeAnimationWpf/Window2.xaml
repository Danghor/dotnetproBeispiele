<Window x:Class="StrokeAnimationWpf.Window2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StrokeAnimationWpf"
        xmlns:Interactions="http://schemas.microsoft.com/xaml/behaviors"
        xmlns:AnimationLib="clr-namespace:StrokeAnimationLib;assembly=StrokeAnimationLib"
        mc:Ignorable="d"
        Title="Window2" Height="450" Width="800">
    <Grid>
    <Ellipse Stroke="Black" StrokeThickness="2" StrokeDashArray="5" Margin="30" x:Name="E1">
      <Interactions:Interaction.Behaviors>
        <AnimationLib:ShapeAnimationBehavior AnimationDirection="Forward"/>
      </Interactions:Interaction.Behaviors>

    </Ellipse>

  </Grid>
</Window>
